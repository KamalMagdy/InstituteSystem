<p id="notice"><%= notice %></p>

<h1>Uploaded Assignments</h1>

<table>
  <thead>
    <tr>
      <th >Student Name</th>
      <th >Uploaded assignment (File)</th>
      <th >Uploaded assignment (Link)</th>
      <th >Course Name</th>
      <th >Track Name</th>
      <th >Delivered Time</th>
      <th >Actions</th>
    </tr>
  </thead>

  <tbody>
  <% @assignmentstaffstudents.each_with_index do |assignmentstaffstudent,index| %>
      <tr>
      <% @assignmentstaffstudentt = Assignmentstaffstudent.find(@assignmentstaffstudents[index]["id"]) %>
      <td><%= @assignmentstaffstudentt.student.name %></td>
      <% if @assignmentstaffstudentt.file_url==nil %>
      <td> </td>
      <% else %>
      <td><%= link_to "Download", @assignmentstaffstudentt.file_url %></td>
      <% end %>
      <td><a href='<%=  @assignmentstaffstudentt.derlivered_assignment %>'><%=  @assignmentstaffstudentt.derlivered_assignment %></a></td>   
      <td><%= @assignmentstaffstudentt.course.name %></td>
      <td><%= @trackarray[index] %></td>
      <td><%= @assignmentstaffstudentt.created_at %></td>
      <% if @assignmentstaffstudentt.codeReview==nil %> 
      <td><%= link_to 'Review', @assignmentstaffstudentt %></td>
      <% else %>
      <td><%= link_to 'Reviewied', @assignmentstaffstudentt %></td>
      <% end %>
      </tr>
      <% end %>
  </tbody>
</table>

<br>

