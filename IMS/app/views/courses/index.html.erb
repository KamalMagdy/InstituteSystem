<p id="notice"><%= notice %></p>

<h1>Courses</h1>


<table>
  <thead>
    <tr>
      <th>Course Name</th>
      <th>Action </th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each_with_index do |course, index| %>
      <tr>
        <td><%= @coursenames[index] %></td>
        <td><%= link_to 'Show', course_path(@courseids[index]) %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

