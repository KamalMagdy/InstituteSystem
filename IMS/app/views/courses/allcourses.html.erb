<h4>List all courses and instructors</h4>
<table>
  <thead>
    <tr>
      <th>Course Name</th>
      <th>Instructor Name</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
  <% @coursenames.each_with_index do |course_name, index| %>
    <tr>
      <td><%= @coursenames[index] %></td>
      <td><%= @instructornames[index] %></td>
      <td><%= link_to 'Edit', edit_course_path(@courseobjects[index]) %></td>
      <td><%= link_to 'Destroy', @courseobjects[index], method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>